"*":
  "autocomplete-plus": {}
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "linter-jshint"
    ]
    openEmptyEditorOnStart: false
    themes: [
      "one-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
  docblockr: {}
  editor:
    invisibles: {}
    preferredLineLength: 120
    scrollPastEnd: true
    showIndentGuide: true
    tabLength: 4
    zoomFontWhenCtrlScrolling: false
  emmet: {}
  "exception-reporting":
    userId: "87517ec7-08bc-4ba9-bb05-cef6652759b2"
  "git-history":
    showDiff: false
  linter:
    lintOnFlyInterval: 500
    showErrorPanel: false
  "linter-jshint": {}
  "project-manager":
    closeCurrent: true
    environmentSpecificProjects: true
  react: {}
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  "vim-mode-plus":
    incrementalSearch: true
    showHoverSearchCounter: true
    useClipboardAsDefaultRegister: true
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
    ignoreWhitespaceOnCurrentLine: false
  "linter-phpcs":
    executablePath: "/usr/bin/phpcs"
".js.jsx.source":
  editor:
    preferredLineLength: 120
".coffee.jsx.source":
  editor:
    preferredLineLength: 120
".html.php.text":
  editor:
    preferredLineLength: 120
".hyperlink.text":
  editor:
    preferredLineLength: 120
